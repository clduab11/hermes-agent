{
  "mcpServers": {
    "supabase": {
      "type": "supabase",
      "url": "https://api.supabase.io",
      "authTokenEnv": "SUPABASE_SERVICE_ROLE_KEY",
      "requiresAuth": true,
      "capabilities": [
        "database_operations",
        "tenant_isolation",
        "conversation_caching",
        "performance_analytics"
      ],
      "timeout": 30,
      "priority": 10
    },
    "mem0": {
      "type": "mem0",
      "url": "https://api.mem0.ai",
      "authTokenEnv": "MEM0_API_KEY",
      "requiresAuth": true,
      "capabilities": [
        "knowledge_graph",
        "entity_management",
        "relationship_tracking",
        "learning_from_interactions"
      ],
      "timeout": 30,
      "priority": 8
    },
    "github": {
      "type": "github",
      "url": "https://api.github.com",
      "authTokenEnv": "GITHUB_TOKEN",
      "requiresAuth": true,
      "capabilities": [
        "repository_management",
        "documentation_generation",
        "version_control",
        "automated_workflows"
      ],
      "timeout": 30,
      "priority": 8
    },
    "puppeteer": {
      "type": "puppeteer",
      "url": "http://localhost:9222",
      "capabilities": [
        "browser_automation",
        "accessibility_testing",
        "ui_validation",
        "form_processing"
      ],
      "timeout": 60,
      "priority": 6
    },
    "mcp-omnisearch": {
      "type": "omnisearch",
      "url": "http://localhost:8080",
      "capabilities": [
        "multi_provider_search",
        "legal_research",
        "precedent_lookup",
        "search_aggregation"
      ],
      "timeout": 60,
      "priority": 6
    },
    "sequential-thinking": {
      "type": "sequential_thinking",
      "url": "http://localhost:9000",
      "capabilities": [
        "complex_decision_trees",
        "legal_compliance_reasoning",
        "multi_step_analysis",
        "workflow_orchestration"
      ],
      "timeout": 90,
      "priority": 5
    },
    "redis": {
      "type": "redis",
      "url": "redis://redis:6379/0",
      "capabilities": [
        "pubsub",
        "caching",
        "rate_limiting"
      ],
      "timeout": 15,
      "priority": 9
    }
  },
  "health": {
    "checkIntervalSeconds": 30,
    "unhealthyThreshold": 3,
    "metricsEndpoint": "/metrics"
  },
  "compliance": {
    "hipaaCompliant": true,
    "gdprCompliant": true,
    "dataRetentionDays": 90,
    "auditLogEnabled": true
  }
}
